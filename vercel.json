{
  "rewrites": [
    {
      "source": "/culturavpn/:path*",
      "destination": "http://culturavpn.site:444/culturavpn:path*"
    }
  ],
  "headers": [
    {
      "source": "/culturavpn/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, max-age=0, must-revalidate" }
      ]
    }
  ]
}
